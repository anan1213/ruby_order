<h1 class="mt-5">ADD CATEGORY</h1>
<%= form_tag("/urls/create_category") do %>
  <div class="table mt-xl-5">
    <ul class="list-group">
      <li class="list-group-item" bg-dark text-white>カテゴリー</li>
      <input name="name" value="<%= @name %>" class="form-control">
    </ul>
  </div>
  <div class="text-right">
  <button type="submit" class="btn btn-outline-dark" >送信</button>
</div>
  <% end %>
</form>
